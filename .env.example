# Remote config endpoint for BrowserOS server settings
BROWSEROS_CONFIG_URL=

# API key for LLM access used by Codex
BROWSEROS_API_KEY=
BROWSEROS_LLM_BASE_URL=
BROWSEROS_LLM_MODEL_NAME=

# Path to codex binary executable
CODEX_BINARY_PATH=third_party/bin/codex

# Server Ports
CDP_PORT=9000
HTTP_MCP_PORT=9100
AGENT_PORT=9200
EXTENSION_PORT=9300

# Agent Configuration
MAX_SESSIONS=5
SESSION_IDLE_TIMEOUT_MS=90000
EVENT_GAP_TIMEOUT_MS=60000

BROWSEROS_BINARY=/Applications/BrowserOS.app/Contents/MacOS/BrowserOS

# PostHog 
POSTHOG_API_KEY=
POSTHOG_ENDPOINT=

LOG_LEVEL=info
NODE_ENV=development
